{
  "name": "mai-ds-2",
  "type": "docker",
  "dockerfile": "Dockerfile",
  "buildContext": ".",
  "ports": [
    { "containerPort": 80, "protocol": "tcp" }
  ],
  "environment": {
    "NODE_ENV": "production"
  },
  "healthCheck": {
    "enabled": true,
    "path": "/",
    "interval": 30,
    "timeout": 10,
    "retries": 3
  },
  "resources": {
    "memory": "512Mi",
    "cpu": "0.5"
  }
}
