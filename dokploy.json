{
  "name": "mai-ds-2",
  "type": "docker",
  "dockerfile": "Dockerfile",
  "buildContext": ".",
  "ports": [
    {
      "containerPort": 3000,
      "hostPort": 80,
      "protocol": "tcp"
    }
  ],
  "environment": {
    "NODE_ENV": "production",
    "PORT": "3000"
  },
  "healthCheck": {
    "enabled": true,
    "path": "/",
    "interval": 30,
    "timeout": 10,
    "retries": 3
  },
  "resources": {
    "memory": "512Mi",
    "cpu": "0.5"
  }
}
