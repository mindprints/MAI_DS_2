{
  "name": "mai_ds_2",
  "version": "1.0.0",
  "description": "A modern, responsive website for the Museum of Artificial Intelligence featuring interactive slideshows, smooth animations, and a professional designnode tools/export-content.js --out-dir exports .",
  "main": "src/site/journal.js",
  "scripts": {
    "build": "npm run build:static && npm run build:pages && npm run build:css",
    "build:static": "node tools/build-static.js",
    "build:css": "postcss src/tailwind.css -o public/assets/css/tailwind.css --env production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "audit": "lighthouse http://localhost:3000 --output=json --output-path=reports/lighthouse.json",
    "optimize": "node tools/optimize-images.js src/site/images/slide",
    "generate:slides": "node tools/generate-slides-manifest.js",
    "build:pages": "node tools/build-pages.js",
    "admin": "node admin/server.js",
    "export-db": "node tools/export-db-content.js",
    "check-db": "node tools/db_reader.js",
    "import-db:dry": "node tools/import_exports.js ./exports --dry",
    "import-db": "node tools/import_exports.js ./exports"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "lighthouse": "^12.0.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "sharp": "^0.33.4",
    "tailwindcss": "^3.4.4"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "pg": "^8.16.3"
  },
  "directories": {
